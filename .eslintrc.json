{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "func-names": 0,
        "new-cap": 0,
        "max-len": ["error", 160],
        "comma-dangle": 0,
        "no-console": "off",
        "padded-blocks": 0,
        "arrow-body-style": "off",
        "no-nested-ternary": "off",
        "camelcase": 0,
        "operator-linebreak": "off"
    }
}
